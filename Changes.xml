  <Version rdf:about="http://search.cpan.org/dist/RDF-RDFa-Parser/#v_0_22">
    <dcs:changeset>
      <rdf:Description>
        <dcs:item>
          <dcs:Bugfix>
            <rdfs:label xml:lang="en">Various test case compliance fixes (passes HTML4 and HTML5 test suites)</rdfs:label>
          </dcs:Bugfix>
        </dcs:item>
        <dcs:item>
          <dcs:Addition>
            <rdfs:label xml:lang="en">Flexible keyword extensibility (optional).</rdfs:label>
          </dcs:Addition>
        </dcs:item>
        <dcs:item>
          <dcs:Addition>
            <rdfs:label xml:lang="en">Special rel='alternate stylesheet' handling (optional, good for HTML5).</rdfs:label>
          </dcs:Addition>
        </dcs:item>
        <dcs:item>
          <dcs:Addition>
            <rdfs:label xml:lang="en">Allow suffix-less CURIEs without colons (optional).</rdfs:label>
          </dcs:Addition>
        </dcs:item>
        <dcs:item>
          <dcs:Addition>
            <rdfs:label xml:lang="en">Allow case-insensitive CURIE prefixes (optional, good for HTML).</rdfs:label>
          </dcs:Addition>
        </dcs:item>
        <dcs:item>
          <dcs:Addition>
            <rdfs:label xml:lang="en">Empty CURIE prefix can be configured (optional).</rdfs:label>
          </dcs:Addition>
        </dcs:item>
        <dcs:item>
          <dcs:Addition>
            <rdfs:label xml:lang="en">Add Auto Config feature.</rdfs:label>
          </dcs:Addition>
        </dcs:item>
        <dcs:item>
          <dcs:Addition>
            <rdfs:label xml:lang="en">Make xml:lang support optional (enabled by default).</rdfs:label>
          </dcs:Addition>
        </dcs:item>
        <dcs:versus rdf:resource="http://search.cpan.org/dist/RDF-RDFa-Parser/#v_0_21"/>
      </rdf:Description>
    </dcs:changeset>
