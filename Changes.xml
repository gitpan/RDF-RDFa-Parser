  <Version rdf:about="http://search.cpan.org/dist/RDF-RDFa-Parser/#v_0_20">
    <dcs:changeset>
      <rdf:Description>
        <dcs:item>
          <dcs:Update>
            <rdfs:label xml:lang="en">The graph() and graphs() methods now return RDF::Trine::Model objects.</rdfs:label>
          </dcs:Update>
        </dcs:item>
        <dcs:item>
          <dcs:Addition>
            <rdfs:label xml:lang="en">Allow various parsing options to be passed to constructor.</rdfs:label>
          </dcs:Addition>
        </dcs:item>
        <dcs:item>
          <rdf:Description>
            <rdfs:label xml:lang="en">Support for @prefix now optional, off by default.</rdfs:label>
          </rdf:Description>
        </dcs:item>
        <dcs:item>
          <rdf:Description>
            <rdfs:label xml:lang="en">Support for @lang now optional, off by default.</rdfs:label>
          </rdf:Description>
        </dcs:item>
        <dcs:item>
          <rdf:Description>
            <rdfs:label xml:lang="en">Support for extended @rel/@rev now optional, off by default.</rdfs:label>
          </rdf:Description>
        </dcs:item>
        <dcs:item>
          <rdf:Description>
            <rdfs:label xml:lang="en">Support for HEAD, BODY and BASE elements now optional, on by default.</rdfs:label>
          </rdf:Description>
        </dcs:item>
        <dcs:item>
          <rdf:Description>
            <rdfs:label xml:lang="en">Support for full URIs instead of CURIEs now optional, off by default.</rdfs:label>
          </rdf:Description>
        </dcs:item>
        <dcs:item>
          <dcs:Addition>
            <rdfs:label xml:lang="en">Add optional support for xml:base.</rdfs:label>
          </dcs:Addition>
        </dcs:item>
        <dcs:item>
          <dcs:Addition>
            <rdfs:label xml:lang="en">Add optional support for embedded RDF/XML.</rdfs:label>
          </dcs:Addition>
        </dcs:item>
        <dcs:item>
          <dcs:Addition>
            <rdfs:label xml:lang="en">Add optional support for XFN 1.1 keywords.</rdfs:label>
          </dcs:Addition>
        </dcs:item>
        <dcs:item>
          <dcs:Removal>
            <rdfs:label xml:lang="en">Diallow Safe CURIEs in @src/@href.</rdfs:label>
          </dcs:Removal>
        </dcs:item>
        <dcs:item>
          <dcs:Update>
            <rdfs:label xml:lang="en">Module SVN repository now at &lt;http://goddamn.co.uk/viewvc/perlmods/&gt;</rdfs:label>
            <rdfs:seeAlso rdf:resource="http://goddamn.co.uk/viewvc/perlmods/"/>
          </dcs:Update>
        </dcs:item>
        <dcs:versus rdf:resource="http://search.cpan.org/dist/RDF-RDFa-Parser/#v_0_11"/>
      </rdf:Description>
    </dcs:changeset>
