my:v_1-00_02

	a               :Version ;
	dc:issued       "2010-02-05"^^xsd:date ;
	:revision       "1.00_02"^^xsd:string ;
	:file-release   <http://backpan.cpan.org/authors/id/T/TO/TOBYINK/RDF-RDFa-Parser-1.00_02.tar.gz> ;
