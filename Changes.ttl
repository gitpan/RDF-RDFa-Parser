my:v_0-04

	a               :Version ;
	dc:issued       "2009-01-14"^^xsd:date ;
	:revision       "0.04"^^xsd:string ;
	:file-release   <http://backpan.cpan.org/authors/id/T/TO/TOBYINK/RDF-RDFa-Parser-0.04.tar.gz> ;
