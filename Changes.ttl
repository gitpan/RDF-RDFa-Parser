my:v_1-00

	a               :Version ;
	dc:issued       "2010-02-18"^^xsd:date ;
	:revision       "1.00"^^xsd:string ;
	:file-release   <http://backpan.cpan.org/authors/id/T/TO/TOBYINK/RDF-RDFa-Parser-1.00.tar.gz> ;
